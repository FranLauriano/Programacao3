		<navigation-case>
			<from-outcome>deleteAcs</from-outcome>
			<to-view-id>/acs/deleteAcs.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>searchAcs</from-outcome>
			<to-view-id>/acs/searchAcs.xhtml</to-view-id>
