<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	
	<lifecycle>
	   <phase-listener>prefeitura.siab.util.AutenticacaoPhase</phase-listener>
	</lifecycle>

	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
	</application>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>updateRaca</from-outcome>
			<to-view-id>/raca/updateRaca.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>deleteRaca</from-outcome>
			<to-view-id>/raca/deleteRaca.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>searchRaca</from-outcome>
			<to-view-id>/raca/searchRaca.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
		
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>updateUsuario</from-outcome>
			<to-view-id>/usuario/updateUsuario.xhtml</to-view-id>
