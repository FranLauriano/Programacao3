	     <redirect></redirect>
	   </navigation-case>
	 </navigation-rule>
	 
	 <navigation-rule>
	   <from-view-id>/*</from-view-id>
	   <navigation-case>
	     <from-outcome>403</from-outcome>
	     <to-view-id>/403.xhtml</to-view-id>
	     <redirect></redirect>
	   </navigation-case>
	 </navigation-rule>
	 
	 <navigation-rule>
	   <from-view-id>/*</from-view-id>
	   <navigation-case>
	     <from-outcome>404</from-outcome>
	     <to-view-id>/404.xhtml</to-view-id>
	     <redirect></redirect>
	   </navigation-case>
	 </navigation-rule>
	 
	 <navigation-rule>
	   <from-view-id>/*</from-view-id>
	   <navigation-case>
	     <from-outcome>500</from-outcome>
	     <to-view-id>/500.xhtml</to-view-id>
	     <redirect></redirect>
	   </navigation-case>
	 </navigation-rule>

</faces-config>
