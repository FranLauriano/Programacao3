	</navigation-rule>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>searchEndereco</from-outcome>
			<to-view-id>/endereco/searchEndereco.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>


	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>updateEscolaridade</from-outcome>
			<to-view-id>/escolaridade/updateEscolaridade.xhtml</to-view-id>
