		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>inicio</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
	   <from-view-id>/*</from-view-id>
	   <navigation-case>
	     <from-outcome>loginPage</from-outcome>
	     <to-view-id>/login/login.xhtml</to-view-id>
	     <redirect></redirect>
	   </navigation-case>
	 </navigation-rule>
	 
	 <navigation-rule>
	   <from-view-id>/*</from-view-id>
